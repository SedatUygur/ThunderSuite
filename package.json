{
  "name": "thundersuite",
  "version": "1.0.0",
  "description": "ThunderSuite is a front-end framework from scratch",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build/ && parcel build index.js -d build/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "rm -rf .cache && rm -rf dist && parcel serve index.html",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SedatUygur/ThunderSuite.git"
  },
  "keywords": [
    "ThunderSuite",
    "thundersuite",
    "js",
    "javascript",
    "framework",
    "Framework",
    "js framework",
    "javascript framework",
    "frontend framework",
    "front-end framework",
    "front end framework",
    "Frontend framework",
    "Front-end framework",
    "Front end framework",
    "frontend",
    "front-end",
    "front end"
  ],
  "author": "Sedat Uygur <sedat.uygur@outlook.com> (https://github.com/SedatUygur)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SedatUygur/ThunderSuite/issues"
  },
  "homepage": "https://github.com/SedatUygur/ThunderSuite#readme",
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "globals": "^15.13.0",
    "parcel": "^2.13.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "snabbdom": "^3.6.2"
  }
}
